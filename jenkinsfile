pipeline{
    agent any
    stages{
        stage('checkout code') {
            steps{
                git url: 'https://github.com/hfirdhows/My_Jenkins25.git', branch: 'main'

            }

        stage('build') {
            steps{
                echo 'Building images'
            }
        } 
        stage('test')  {
            steps{
                echo 'test pass'
            }
        }
        stage('deploy') {
            steps{
                echo 'application deployed'
            }
        }
        }
    }
}
